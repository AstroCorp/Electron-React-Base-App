{
  "name": "electron-react-base",
  "description": "Electron + React",
  "author": "AstroCorp",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "react-start": "craco start",
    "react-build": "craco build",
    "react-test": "craco test --env=jsdom",
    "react-eject": "craco eject",
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "clean": "rm -rf build/ && rm -rf dist/ && rm -rf node_modules/ && rm yarn.lock && yarn cache clean && yarn install"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "connected-react-router": "^6.9.1",
    "electron-is-dev": "2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.1",
    "redux-axios-middleware": "^4.0.1",
    "update-electron-app": "^2.0.1"
  },
  "devDependencies": {
    "@craco/craco": "^6.3.0",
    "@tailwindcss/postcss7-compat": "^2.2.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.1.8",
    "@types/webpack-env": "^1.16.2",
    "autoprefixer": "^9.8.6",
    "concurrently": "^6.2.1",
    "cross-env": "7.0.3",
    "electron": "^14.0.0",
    "electron-builder": "^22.11.7",
    "electron-log": "^4.4.1",
    "prettier": "^2.3.2",
    "react-scripts": "4.0.3",
    "postcss": "^7.0.36",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@2.2.10",
    "typescript": "^4.4.2",
    "wait-on": "^6.0.0"
  },
  "build": {
    "publish": {
      "provider": "github"
    }
  },
  "main": "public/electron.js",
  "homepage": "./",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
